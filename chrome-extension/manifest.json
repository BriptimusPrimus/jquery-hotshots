{
    "name": "Web Contacts",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Scrape web pages for Schema.org micro-data",
    "browser_action": {
        "default_popup": "popup.html"
    },
	"sandbox": {
	    "pages": ["template.html"]
	},
    "content_scripts": [{
        "matches": ["*://*/*"],
        "css": ["content.css"],
        "js": ["jquery-2.1.1.min.js", "content.js"]
    }],
    "background": {
        "scripts": ["jquery-2.1.1.min.js", "background.js"]
    }    
}